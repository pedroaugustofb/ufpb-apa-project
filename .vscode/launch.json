{
  "configurations": [
    {
      "name": "C/C++: g++.exe Compilar e depurar o arquivo ativo",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
      "args": [
        "f:\\UFPB\\APA\\project\\entities\\job.cpp",
        "f:\\UFPB\\APA\\project\\entities\\server.cpp"
      ],
      "stopAtEntry": false,
      "cwd": "C:/Code/Ambiente/MinGW/bin",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:\\Code\\Ambiente\\MinGW\\bin\\gdb.exe",
      "setupCommands": [
        {
          "description": "Habilitar a reformatação automática para gdb",
